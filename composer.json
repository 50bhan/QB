{
    "name": "sharifi/laravel-exception-handler",
    "description": "",
    "keywords": [
        "laravel",
        "framework",
        "exceptions",
        "exception",
        "errors",
        "error",
        "whoops",
        "Exceptions",
        "Laravel Exceptions",
        "Laravel-Exception-handler",
        "Exception Handler"
    ],
    "support": {
        "issues": "https://github.com/50bhan/laravel-exception-handler/issues",
        "source": "https://github.com/50bhan/laravel-exception-handler"
    },
    "authors": [
        {
            "name": "Sobhan Sharifi",
            "email": "sobhan.sharifi.sadeghi@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.3",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Sharifi\\Exceptions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sharifi\\Exceptions\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text",
        "test-coverage": "vendor/bin/php-coveralls -v"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sharifi\\Exceptions\\ServiceProvider"
            ]
        }
    },
    "config": {
        "discard-changes": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
